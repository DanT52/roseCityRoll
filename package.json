{
  "name": "rosecityroll",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2"
  },
  "scripts": {
    "dev": "concurrently \"cd frontend && npm run dev\" \"cd backend && python main.py\"",
    "lint-front": "cd frontend && eslint .",
    "lint-back": "pylint backend",
    "lint-all": "npm run lint-front && npm run lint-back",
    "prepare": "husky"
  },
  "lint-staged": {
    "frontend/**/*.{ts,tsx}": [
      "frontend/node_modules/.bin/eslint --fix --config frontend/eslint.config.js"
    ],
    "backend/**/*.py": [
      "pylint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
